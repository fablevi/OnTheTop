<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.on-the-top" path="/org/gnome/shell/extensions/on-the-top/">
    <key name="positions" type="s">
      <choices>
        <choice value="left"/>
        <choice value="right"/>
      </choices> 
      <default>'right'</default>
      <summary>Position settings</summary>
      <description>This specifies where is the icon will be shown.</description>
      </key>
    <key name="ranking" type="s">
      <choices>
        <choice value="0"/>
        <choice value="1"/>
        <choice value="2"/>
        <choice value="3"/>
        <choice value="4"/>
        <choice value="5"/>
        <choice value="6"/>
        <choice value="7"/>
        <choice value="8"/>
        <choice value="9"/>
      </choices> 
      <default>'2'</default>
      <summary>Ranking settings</summary>
      <description>This specifies where is the icon will be shown.</description>
      </key>
  </schema>
</schemalist>
